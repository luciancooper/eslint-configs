{
  "name": "@lcooper/eslint-config",
  "version": "3.1.0",
  "description": "Shareable config for ESLint",
  "main": "./cjs.js",
  "types": "./types/configs.d.ts",
  "exports": {
    ".": {
      "types": "./types/configs.d.ts",
      "import": "./esm.js",
      "require": "./cjs.js",
      "default": "./base.js"
    },
    "./base": {
      "types": "./types/configs.d.ts",
      "default": "./base.js"
    },
    "./cjs": {
      "types": "./types/configs.d.ts",
      "default": "./cjs.js"
    },
    "./esm": {
      "types": "./types/configs.d.ts",
      "default": "./esm.js"
    },
    "./envs": {
      "types": "./types/envs.d.ts",
      "default": "./envs.js"
    },
    "./rules/core": {
      "types": "./types/config.d.ts",
      "default": "./rules/core.js"
    },
    "./rules/import": {
      "types": "./types/config.d.ts",
      "default": "./rules/import.js"
    },
    "./rules/jsdoc": {
      "types": "./types/config.d.ts",
      "default": "./rules/jsdoc.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "base": [
        "./types/configs.d.ts"
      ],
      "cjs": [
        "./types/configs.d.ts"
      ],
      "esm": [
        "./types/configs.d.ts"
      ],
      "envs": [
        "./types/envs.d.ts"
      ],
      "rules/core": [
        "./types/config.d.ts"
      ],
      "rules/import": [
        "./types/config.d.ts"
      ],
      "rules/jsdoc": [
        "./types/config.d.ts"
      ]
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint:types": "attw --pack"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "base.js",
    "cjs.js",
    "esm.js",
    "envs.js",
    "rules",
    "types"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/eslint-configs.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/luciancooper/eslint-configs/issues"
  },
  "homepage": "https://github.com/luciancooper/eslint-configs/tree/main/packages/eslint-config#readme",
  "dependencies": {
    "@lcooper/eslint-plugin": "^1.5.0",
    "@stylistic/eslint-plugin-js": "^3.1.0",
    "@stylistic/eslint-plugin-plus": "^3.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.9",
    "globals": "^14.0.0"
  },
  "peerDependencies": {
    "eslint": ">=9.3"
  }
}
