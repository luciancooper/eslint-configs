{
  "name": "@lcooper/eslint-config",
  "version": "3.0.0",
  "description": "Shareable config for ESLint",
  "exports": {
    ".": {
      "import": "./esm.js",
      "require": "./cjs.js",
      "default": "./base.js"
    },
    "./base": "./base.js",
    "./envs": "./envs.js",
    "./cjs": "./cjs.js",
    "./esm": "./esm.js",
    "./rules/*": "./rules/*.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "base.js",
    "cjs.js",
    "esm.js",
    "envs.js",
    "rules"
  ],
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/eslint-configs.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/luciancooper/eslint-configs/issues"
  },
  "homepage": "https://github.com/luciancooper/eslint-configs/tree/main/packages/eslint-config#readme",
  "dependencies": {
    "@lcooper/eslint-plugin": "^1.4.0",
    "@stylistic/eslint-plugin-js": "^3.1.0",
    "@stylistic/eslint-plugin-plus": "^3.1.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^48.2.1",
    "globals": "^14.0.0"
  },
  "peerDependencies": {
    "eslint": ">=9.3"
  }
}
